    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">= 3.0.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "extract-text-webpack-plugin",
      "karma-webpack",
      "webpack",
      "webpack-merge"
    ]
  }
