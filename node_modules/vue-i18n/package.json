    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "buble": "^0.19.3",
    "chromedriver": "^88.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
