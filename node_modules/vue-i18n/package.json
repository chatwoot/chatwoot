    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "selenium-server": "^3.141.59",
    "shipjs": "^0.22.0",
    "sinon": "^9.0.2",
    "terser": "^3.17.0",
    "typescript": "^3.9.3",
    "vue": "^2.5.17",
    "vue-github-button": "^1.1.2",
