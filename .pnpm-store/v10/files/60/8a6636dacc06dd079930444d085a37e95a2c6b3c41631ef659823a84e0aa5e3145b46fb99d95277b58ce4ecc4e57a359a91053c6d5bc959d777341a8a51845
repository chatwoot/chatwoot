#ifndef STDLIB_CONSTANTS_FLOAT64_MAX_BASE2_EXPONENT_SUBNORMAL_H
#define STDLIB_CONSTANTS_FLOAT64_MAX_BASE2_EXPONENT_SUBNORMAL_H

/**
* Macro for the maximum biased base 2 exponent for a subnormal double-precision floating-point number.
*/
#define STDLIB_CONSTANT_FLOAT64_MAX_BASE2_EXPONENT_SUBNORMAL -1023

#endif // !STDLIB_CONSTANTS_FLOAT64_MAX_BASE2_EXPONENT_SUBNORMAL_H
