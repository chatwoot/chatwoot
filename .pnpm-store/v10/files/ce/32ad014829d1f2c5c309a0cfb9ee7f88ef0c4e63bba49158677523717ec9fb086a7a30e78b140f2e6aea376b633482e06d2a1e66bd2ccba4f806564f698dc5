  <div class='clearfix'>
    
    <h3 class='fl m0' id='type'>
      type
    </h3>
    
    
      <a class='fr fill-darken0 round round pad1x quiet h5' href='https://github.com/segmentio/facade/blob/f69b5795421c407210f4b414453ebe5ebd6379a5/lib/identify.js#L46-L46'>
