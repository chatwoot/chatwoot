  return (\n          typeof (plugin as FormKitPlugin & { iconHandler: FormKitIconLoader })\n            .iconHandler === 'function'\n        )\n      }) as (FormKitPlugin & { iconHandler: FormKitIconLoader }) | undefined\n      if (iconPlugin) {\n        iconHandler = iconPlugin.iconHandler\n      }\n    }\n\n    watch(\n      () => props.icon,\n      () => {\n        loadIcon()\n      },\n      { immediate: true }\n    )\n\n    return () => {\n      if (props.icon && icon.value) {\n        return h('span', {\n          class: 'formkit-icon',\n          innerHTML: icon.value,\n        })\n      }\n      return null\n    }\n  },\n})\n\nexport default FormKitIcon\n","/**\n * Reset any counters in the inputs package.\n */\nimport { resetCounts } from '@formkit/inputs'\nimport { resetCount as coreResetCount } from '@formkit/core'\n\nexport function resetCount() {\n  resetCounts()\n  coreResetCount()\n}\n"]}